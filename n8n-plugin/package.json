{
  "name": "n8n-nodes-jianying-draft",
  "version": "1.0.0",
  "description": "n8n node for managing JianyingPro draft files based on pyJianYingDraftServer API",
  "keywords": [
    "n8n-community-node-package",
    "jianying",
    "jianyingpro",
    "video-editing",
    "draft"
  ],
  "license": "MIT",
  "homepage": "https://github.com/your-username/n8n-nodes-jianying-draft",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/n8n-nodes-jianying-draft.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/JianyingDraftApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/JianyingDraft/ReadPreset.node.js",
      "dist/nodes/JianyingDraft/ImportDraft.node.js",
      "dist/nodes/JianyingDraft/GetTaskResult.node.js",
      "dist/nodes/JianyingDraft/SubmitDraftWithUrl.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "dependencies": {}
}
